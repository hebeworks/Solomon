{{#stories/story-base 
	title=title 
	subTitle=subTitle 
	color="white" 
	width="2"
	height="2" 
	data-ss-colspan="2"}}
    
		<main class="ct-wrapper">
			{{yield}}
				{{charts/chartist-base 
					data=chartData
					type="bar"
					options=chartOptions
					ratio="ct-story_2-2"
					chart=chart}}
		</main>
		
{{/stories/story-base}}
